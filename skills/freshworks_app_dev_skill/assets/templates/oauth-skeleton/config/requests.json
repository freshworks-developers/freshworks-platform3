{
  "getOAuthResource": {
    "schema": {
      "method": "GET",
      "host": "api.github.com",
      "path": "/repos/<%= iparam.repo_name %>",
      "headers": {
        "Authorization": "bearer <%= access_token %>",
        "Accept": "application/vnd.github.v3+json"
      }
    },
    "options": {
      "oauth": "github"
    }
  }
}
